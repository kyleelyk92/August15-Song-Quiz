@model List<Question2WebApplication.Controllers.Songresult>
@{
    ViewBag.Title = "UserSongs";
}

<h2>UserSongs</h2>

@if (Model != null)
{
    <h3>User Song List</h3>
    <ul>
        @foreach (var item in Model)
        {
            <li>
                @item.Title
            </li>
            <li>
                @item.Rating
            </li>
        }
    </ul>

}